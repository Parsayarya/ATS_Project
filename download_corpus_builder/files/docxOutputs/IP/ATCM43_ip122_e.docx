    <script>
        $(document).ready(function () {
            var menuIsShown = false;
            var menu = "";

            $('[data-menu]').on('click', function (e) {
                e.preventDefault();

                menu = $(this).attr('data-menu');
                $('#' + menu + '> ul > li').show();

                $('#' + menu).fadeIn().addClass('show').css({
                    top: 35,
                    left: e.pageX - 5
                });

                menuIsShown = true;
            });

            $(document).mousemove(function () {
                if (menuIsShown) {
                    setTimeout(() => {
                        if ($('#' + menu).is(':hover') == false) {
                            $('#' + menu).fadeOut().removeClass('show');
                            menuIsShown = false;
                        };
                    }, 300);
                }
            });
        });
    </script>

    <script type="text/javascript">
        function goBackToATSIdentity() {
             $.ajax({
                url: `/devAS/Ats/BackToATSIdentity`,
                type: 'GET',
                dataType: 'json',
                success: function (data) {
                    if (data.msg && data.msg == "OK") {
                        window.location.reload();
                    } else {
                        if (data.msg) {
                            alert(data.msg);
                        } else {
                            alert("The server couldn&#39;t complete the request. Try again later.");
                        }
                    }
                }
            });
        }
    </script>
