    <!-- Scripts Section -->
    

    <script type="text/javascript">
        var redirectUrl = "";
        var newTab = false;

        function showLoginPopup() {
            $(".login-popup").show();
        }

        function dismissLogin() {
            $(".login-popup").hide();
        }

        function redirectAfterLogin(url, nTab) {
            redirectUrl = url;
            newTab = nTab;
            showLoginPopup();
        }

        $("#login-form").submit(function (e) {
            e.preventDefault();

            $("#loginMsg").text("");
            var form = $(this);
            var url = form.attr('action');

            $.ajax({
                type: "POST",
                url: url,
                data: form.serialize(),
                success: function (data) {
                    var d = JSON.parse(data);
                    if (d.code == 200) {
                        if (newTab) {
                            window.open(redirectUrl, '_blank');
                        }
                        else {
                            window.location = redirectUrl;
                        }
                    } else {
                        $("#loginMsg").text(d.msg);
                    }

                }
            });
        });

        if (window.location.hash == "#login") {
            var urlParams = new URLSearchParams(location.search);

            if (urlParams.has('returnUrl')) {
                var entries = urlParams.entries();
                for (pair of entries) {
                    if (pair[0] === 'returnUrl') {
                        redirectUrl = pair[1];
                    } else {
                        if (redirectUrl.indexOf("?") > -1) {
                            redirectUrl = redirectUrl + '&' + pair[0] + '=' + pair[1];
                        } else {
                            redirectUrl = redirectUrl + '?' + pair[0] + '=' + pair[1];
                        }
                    }
                }
            }

            $(".login-popup").show();
        }
    </script>


     


